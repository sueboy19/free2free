#GIN_MODE=release

# 資料庫設定
DB_HOST=localhost
DB_USER=free2free_user
DB_PASSWORD=free2free_password
DB_NAME=free2free

# Session 金鑰 (請使用隨機字串，至少32個字符)
SESSION_KEY=this_is_a_very_long_secret_key_that_is_at_least_32_characters_long

# JWT 金鑰 (請使用隨機字串，至少32個字符)
JWT_SECRET=this_is_a_very_long_jwt_secret_key_that_is_at_least_32_characters_long

# Facebook OAuth 設定
FACEBOOK_KEY=your_facebook_app_key
FACEBOOK_SECRET=your_facebook_app_secret

# Instagram OAuth 設定
INSTAGRAM_KEY=your_instagram_app_key
INSTAGRAM_SECRET=your_instagram_app_secret

# 應用程式基礎 URL
BASE_URL=http://localhost:8080

# Cookie 安全性（使用 HTTPS 時設為 true）
SECURE_COOKIE=false

# 資料庫自動遷移（開發時建議設為 true）
AUTO_MIGRATE=true

# 前端 URL（CORS 設定用）
FRONTEND_URL=http://localhost:3000

# Gin 執行模式（debug/release）
#GIN_MODE=release